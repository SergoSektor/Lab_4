<Window x:Class="CardGame.DebugOut"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CardGame"
        mc:Ignorable="d"
        Title="DebugOut" Height="450" Width="379.605" ResizeMode="NoResize" Closed="Window_Closed">
    <Grid>
        <RichTextBox x:Name="LogText" HorizontalAlignment="Left" Height="344" Margin="34,40,0,0" VerticalAlignment="Top" Width="318">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
